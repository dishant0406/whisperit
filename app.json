{
  "expo": {
    "name": "wishperit",
    "slug": "wishperit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ios/iTunesArtwork@2x.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/whisperitt.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.whisperit.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/android/playstore-icon.png",
        "backgroundColor": "#101010"
      },
      "package": "com.whisperit.app"
    },
    "web": {
      "favicon": "./assets/android/ic_launcher-web.png"
    },
    "extra": {
      "eas": {
        "projectId": "108a7a00-d47f-4369-ad15-6faca75fc7f2"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/ios/AppIcon.appiconset/Icon-App-29x29@1x.png",
          "color": "#ffffff"
        }
      ]
    ]
  }
}
